<?xml version="1.0" encoding="utf-8"?>
<!--
  Only nuget.org is used. This is the v3 service index; the client resolves
  package URLs (e.g. v3-flatcontainer/<id>/<version>/<id>.<version>.nupkg) from it.
-->
<configuration>
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>
