{
  "metadata": [
    {
      "src": [
        { "src": "../src", "files": [ "**/*.csproj" ] }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      { "files": [ "**/*.md", "**/toc.yml", "api/*.yml" ] }
    ],
    "resource": [
      { "files": [ "images/**" ] }
    ],
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "templates/intentum"
    ],
    "globalMetadata": {
      "_appName": "Intentum",
      "_appTitle": "Intentum API",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/logo.png",
      "_disableContribution": true,
      "_enableSearch": true
    }
  }
}
